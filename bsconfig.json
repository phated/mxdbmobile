{
  "name": "mxdbmobile",
  "bsc-flags": [
    "-bs-no-version-header",
    "-bs-super-errors"
  ],
  "refmt": 3,
  "bs-dependencies": [
    "reason-react",
    "bs-react-native",
    "@glennsl/bs-json",
    "bs-history",
    "@reasonableconsulting/apollo",
    "@reasonableconsulting/oolong",
    "@reasonableconsulting/compare",
    "@reasonableconsulting/bs-victory",
    "@aantron/repromise"
  ],
  "namespace": true,
  "reason": {
    "react-jsx": 2
  },
  "package-specs": {
    "module": "commonjs",
    "in-source": true
  },
  "suffix": ".bs.js",
  "sources": [
    {
      "dir": "src",
      "subdirs": [
        "icon",
        "card",
        "deck",
        "components",
        "page",
        "charts"
      ]
    }
  ]
}
